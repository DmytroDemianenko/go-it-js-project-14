{{#each this}}
<li class="film-card" >
    <div class="">
        <img class="film-card__img" src="https://image.tmdb.org/t/p/w500{{poster_path}}" alt="{{backdrop_path}}" width="100%" data-id="{{id}}" />
    <h2 class="film-card__title">{{title}} {{name}}</h2>
    <p class="film-card__text">
        <span>{{genre_ids}}</span> 
        | 
        <span>{{release_date}}{{first_air_date}}</span>
        <span class="film-card__vote">{{vote_average}}</span>
    </p>
    </div>
</li>
{{/each}}